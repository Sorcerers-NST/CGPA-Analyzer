generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model students {
  student_id Int @id @default(autoincrement())
  student_name String
  student_email String @unique
  password String
  college_name String
  departement String
  graduation_year Int
}